{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "types": ["solid-js"],
    "allowJs": true,
    "useUnknownInCatchVariables": false,
    "baseUrl": "src",
    "paths": {
      "@lib/*": ["lib/*"],
      "@utils/*": ["utils/*"],
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@assets/*": ["assets/*"],
      "@stores/*": ["stores/*"],
      "@pages/*": ["pages/*"]
      
    }
    
  }
}
